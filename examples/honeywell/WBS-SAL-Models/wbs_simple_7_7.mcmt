	  (cs!331 (/= time_arri!4 (- 1)))
	  (cs!332 (/= time_arri!0 (- 1)))
	  (cs!333 (/= time_arri!1 (- 1)))
	  (cs!334 (/= time_arri!2 (- 1)))
	  (cs!335 (/= time_arri!3 (- 1))))
      (and
       (and
	cs!321
	cs!322
	cs!323
	cs!324
	cs!325
	(or
	 (= time time_arri!0)
	 (= time time_arri!1)
	 (= time time_arri!2)
	 (= time time_arri!3)
	 (= time time_arri!4)))))

    (<= 0 NODE2_PHASE) (< NODE2_PHASE NODE_CLOCK_PERIOD)
    (<= 0 NODE3_PHASE) (< NODE3_PHASE NODE_CLOCK_PERIOD)
    (<= 0 NODE4_PHASE) (< NODE4_PHASE NODE_CLOCK_PERIOD)
