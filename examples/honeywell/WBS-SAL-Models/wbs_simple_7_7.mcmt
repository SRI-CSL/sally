(assume T
   (and 
    (let ((cs!321 (<= time time_arri!0))
	  (cs!322 (<= time time_arri!1))
	  (cs!323 (<= time time_arri!2))
	  (cs!324 (<= time time_arri!3))
	  (cs!325 (<= time time_arri!4))
